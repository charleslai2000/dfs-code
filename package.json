{
  "name": "@nexp/dfs-code",
  "version": "0.1.0",
  "private": true,
  "description": "NexP data graph application.",
  "type": "module",
  "exports": {
    "./env": {
      "import": "./src/env/index.ts"
    },
    "./providers": {
      "import": "./src/providers/index.ts"
    },
    "./packages": {
      "import": "./src/packages.ts"
    }
  },
  "scripts": {
    "check": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist",
    "build": "swc src -d dist/esm",
    "preview": "vite preview",
    "dev": "node --loader @swc-node/register/esm --no-warnings vite-boot.ts",
    "commit": "git add . && { git commit -m \"$COMMIT_MSG\"; } || true",
    "format": "prettier --write . && eslint src/** --fix",
    "lint": "prettier --check . && eslint src/**",
    "pull": "git pull",
    "push": "git push",
    "push-tags": "git push --tags",
    "test": "jest --silent"
  },
  "dependencies": {
